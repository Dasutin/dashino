{
  "name": "dashino",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "predev": "node scripts/sync-controllers.mjs",
    "dev": "concurrently -k -n server,client \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "wait-on http://localhost:4040 && vite --host 0.0.0.0 --config vite.config.ts",
    "prebuild:client": "node scripts/sync-controllers.mjs",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc --project tsconfig.json",
    "build:client": "vite build --config vite.config.ts",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "archiver": "^5.3.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "chart.js": "^4.5.1",
    "concurrently": "^8.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  }
}
